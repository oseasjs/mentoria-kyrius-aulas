    "history": "4.10.1",
    "sass": "^6.0.0",
    "perfect-scrollbar": "1.5.0",
    "prop-types": "15.7.2",
    "react": "16.9.23",
    "react-chartjs-2": "2.9.0",
    "react-dom": "16.9.23",
